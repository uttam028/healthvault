<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
		.important {
		font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel addStyleNames="col-lg-8">
		<b:Panel>
			<b:PanelHeader>
				<b:Heading size="H3" ui:field="playerHeader"/>
			</b:PanelHeader>
			<b:PanelBody>
				<b:Form type="HORIZONTAL" ui:field="formLogin">
					<b:FieldSet>
						<b:FormGroup>
						    <g:HorizontalPanel ui:field="playerButtonPanel">
						    	<!--b:ImageAnchor url="images/record.png"></b:ImageAnchor>
								<b:FormLabel>Record</b:FormLabel-->						        
						    </g:HorizontalPanel>
						</b:FormGroup>
						<b:FormGroup>
							<g:HorizontalPanel>
								<b:Button ui:field="buttonStartOver" color="blue" addStyleNames="col-lg-offset-6 rightPadding">Start Over	</b:Button>
								<b:Button ui:field="buttonUpload" color="blue">Upload</b:Button>
							</g:HorizontalPanel>
						</b:FormGroup>
					</b:FieldSet>
				</b:Form>
			</b:PanelBody>
		</b:Panel>
	</g:HTMLPanel>
</ui:UiBinder> 