<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel addStyleNames="col-lg-offset-1 col-lg-10">
		<h4>Adjust volume if required</h4>
		<g:HTMLPanel ui:field="textPanel">
			<h2>Could you hear me properly?</h2>
		</g:HTMLPanel>
		<g:HTMLPanel ui:field="imagePanel">
			<img src="images/wave.png"/>
		</g:HTMLPanel>
		<br/><br/>	
		<hr/>
		<b:Button ui:field="buttonYes" size="LARGE">Yes</b:Button>
		<b:Button ui:field="buttonNo" size="LARGE">No</b:Button>
	</g:HTMLPanel>
</ui:UiBinder> 