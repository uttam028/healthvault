<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
		.important {
		font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel addStyleNames="col-lg-offset-1 col-lg-10" ui:field="customPlayerPanel">
		<br />
		<br />
		<b:Panel>
			<b:PanelHeader>
				<b:Heading size="H2" ui:field="playerHeader" />
			</b:PanelHeader>


			<b:PanelBody>
				<g:HTMLPanel addStyleNames="panel_border">
					<h:Br />
					<g:HTMLPanel ui:field="commonInstructionPanel">
						<g:Label ui:field="commonInstructionText" addStyleNames="common_inst_text"></g:Label>
					</g:HTMLPanel>
					<h:Br/>
					<g:HTMLPanel ui:field="fragmentInstructionPanel">
					</g:HTMLPanel>
					<h:Br />
					<g:HTMLPanel ui:field="userInterfaceInstructionPanel">
						<b:Button ui:field="buttonStartTest" size="LARGE" type="SUCCESS">Start</b:Button>
					</g:HTMLPanel>
					<h:Br/>

				</g:HTMLPanel>
				<h:Br />
				<b:Row addStyleNames="col-lg-offset-4">
					<b:Button ui:field="buttonStartOver" size="LARGE" type="PRIMARY">Retake The Test</b:Button>
					<b:Button ui:field="buttonUpload" addStyleNames="col-lg-offset-1"
						size="LARGE" type="SUCCESS">Continue</b:Button>
				</b:Row>
			</b:PanelBody>
		</b:Panel>

	</g:HTMLPanel>
</ui:UiBinder> 