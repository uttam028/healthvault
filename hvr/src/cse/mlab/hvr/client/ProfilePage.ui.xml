<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
		.important {
		font-weight: bold;
		}
		.left_text{
			text-align:left;
			font-family: "Times New Roman", Times, serif;
		}
	</ui:style>
	<g:HTMLPanel>
		<b:Column size="MD_5">
		    <h:Br/><h:Br/>
			<g:HTMLPanel addStyleNames="col-lg-offset-5 col-lg-5">
				<b:Panel addStyleNames="{style.left_text}">
					<b:PanelHeader>
						<b:Heading size="H4" text="User Information"/>
					</b:PanelHeader>
					<b:LinkedGroupItem text="Personal" color="BLUE" ui:field="personalInfoLink"/>
					<b:LinkedGroupItem text="Account" color="BLUE" ui:field="accountInfoLink"/>
					<b:LinkedGroupItem text="Medical" color="BLUE" ui:field="medicalInfoLink"/>

				</b:Panel>

			</g:HTMLPanel>

		</b:Column>
		<b:Column size="MD_7">
		    <g:HTMLPanel ui:field="profileContentPanel" addStyleNames="col-lg-10">
		        
		    </g:HTMLPanel>
		</b:Column>
	</g:HTMLPanel>
</ui:UiBinder> 