<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.margin{
			margin:80%;
		}
		.heading_color{
			background-color: #194719 !important;
		}
		
	</ui:style>
	<b:Panel>
		<b:PanelHeader ui:field="tpHeader">
			<b:Heading size="H5" ui:field="tpHeading"/>
		</b:PanelHeader>
		<b:PanelBody>
		    <b:Row>
		        <h:Paragraph ui:field="tpDescription"></h:Paragraph>
		    </b:Row>
		    <b:Row>
		        <b:Column size="MD_8">
		        	<g:HTMLPanel ui:field="tpQuestionAnswerList"></g:HTMLPanel>
		        </b:Column>
		        <b:Column size="MD_4">
		            <b:Button size="LARGE" ui:field="tpStartButton" type="PRIMARY">Begin New Test</b:Button>
		        </b:Column>
		    </b:Row>
		</b:PanelBody>
	</b:Panel>
</ui:UiBinder> 