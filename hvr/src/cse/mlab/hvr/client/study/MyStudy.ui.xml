<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
		.important {
		font-weight: bold;
		}
		.margin{
		margin:5%;
		}
		.heading_color{
		background-color: #194719 !important;
		}
	</ui:style>
	<b:Panel addStyleNames="{style.margin}">
		<b:PanelHeader ui:field="msHeader">
			<b:Heading size="H5" ui:field="msHeading" text="Parkinson Study"/>
		</b:PanelHeader>
		<b:PanelBody>
			<b:Row>
				<h:Paragraph ui:field="msDescription"></h:Paragraph>
			</b:Row>
			<b:Row>
				<b:Column size="MD_6">
				    <g:Label>Enrollment Date: 2016-07-08</g:Label>
				    <h:Br/>
				    <g:Label>Last participation Date: 2016-07-08</g:Label>
				    <h:Br/>
				    <g:Label>Total participation: 02</g:Label>
				    <h:Br/>				    				    
				</b:Column>
				<b:Column size="MD_6">
				    <g:Label>Compliance Score: 40%</g:Label>
				    <h:Br/>
				    <g:Label>A message related to compliance</g:Label>
				    <h:Br/>				    					
				</b:Column>
			</b:Row>
			<h:Hr/>
			<b:Row>
			    <b:Button ui:field="participateButton" type="SUCCESS">Participate</b:Button>
			</b:Row>
		</b:PanelBody>
	</b:Panel>
</ui:UiBinder> 