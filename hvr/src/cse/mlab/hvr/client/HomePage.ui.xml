<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
		.important {
		font-weight: bold;
		}
		.margin{
		margin:5%;
		}
		.text_left{
			text-align: left;
		}

	</ui:style>
	<g:HTMLPanel ui:field="homepagePanel">
		<b:Row>
			<b:Column size="MD_8">
			    <h:Br/>
			    <g:HTMLPanel addStyleNames="col-lg-offset-2 col-lg-9" ui:field="messagePanel"></g:HTMLPanel>
			    <h:Br/><h:Br/>
				<g:HTMLPanel addStyleNames="col-lg-offset-2 col-lg-9" ui:field="dashboardPanel">
				</g:HTMLPanel>
			</b:Column>
			<b:Column size="MD_4">
			    <h:Br/><h:Br/><h:Br/><h:Br/><h:Br/>
				<g:HTMLPanel  addStyleNames="col-lg-offset-1" ui:field="openStudyManagerPanel">
				    <!--g:Label addStyleNames="{style.text_left}">Open Studies</g:Label-->
				    <h3>Studies open for enrollment</h3>
				    <hr/>
				    <g:HTMLPanel ui:field="openStudyPanel" addStyleNames="col-lg-offset-1 col-lg-10"></g:HTMLPanel>				    
				</g:HTMLPanel>
				</b:Column>
		</b:Row>
	</g:HTMLPanel>
</ui:UiBinder> 