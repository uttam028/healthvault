<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	.important {
		font-weight: bold;
	}
	.text_font{
		font-size: 120%;
	}
	
	</ui:style>
	<g:HTMLPanel ui:field="pretestPanel" addStyleNames="panel_border">
		<g:Label addStyleNames="h3">Please answer below questions:</g:Label>
		<br/>
		<g:HTMLPanel ui:field="questionPanel" addStyleNames="{style.text_font}"></g:HTMLPanel>
		<hr/>
		<br/>
		<b:Button ui:field="pretestSubmitButton" size="LARGE" type="PRIMARY">Submit</b:Button>
		<br/><br/><br/>
	</g:HTMLPanel>
</ui:UiBinder> 